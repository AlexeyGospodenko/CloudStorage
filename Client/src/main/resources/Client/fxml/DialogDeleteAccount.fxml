<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="window" prefHeight="100.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.DeleteAccountController">
    <children>
        <Button fx:id="btnYes" layoutX="22.0" layoutY="60.0" mnemonicParsing="false" onAction="#EnterDeleteAccount"
                prefHeight="25.0" prefWidth="60.0" text="Yes" AnchorPane.rightAnchor="90.0"
                AnchorPane.topAnchor="60.0"/>
        <Button fx:id="btnNo" layoutX="212.0" layoutY="61.0" mnemonicParsing="false" onAction="#CancelDeleteAccount"
                prefHeight="25.0" prefWidth="60.0" text="No" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="60.0"/>
        <Label layoutX="35.0" layoutY="14.0" prefHeight="45.0" prefWidth="230.0"
               text="Enter your password if you are sure you want to delete your account" wrapText="true"
               AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="10.0">
            <font>
                <Font size="13.0"/>
            </font>
        </Label>
        <TextField fx:id="txtPassword" layoutY="60.0" prefWidth="170.0" AnchorPane.leftAnchor="15.0"/>
    </children>
</AnchorPane>
