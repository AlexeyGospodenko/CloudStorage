<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="window" prefHeight="100.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.NewFolderController">
    <children>
        <TextField fx:id="txtFolderName" layoutX="14.0" layoutY="14.0" prefHeight="30.0" prefWidth="270.0"
                   text="New Folder" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0"
                   AnchorPane.topAnchor="15.0"/>
        <Button fx:id="btnOk" layoutX="22.0" layoutY="60.0" mnemonicParsing="false" onAction="#EnterFolderName"
                prefHeight="25.0" prefWidth="60.0" text="OK" AnchorPane.rightAnchor="90.0" AnchorPane.topAnchor="60.0"/>
        <Button fx:id="btnCancel" layoutX="212.0" layoutY="61.0" mnemonicParsing="false" onAction="#CancelFolderName"
                prefHeight="25.0" prefWidth="60.0" text="Cancel" AnchorPane.rightAnchor="15.0"
                AnchorPane.topAnchor="60.0"/>
    </children>
</AnchorPane>
